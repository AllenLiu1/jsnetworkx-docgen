{
  "name": "jsnetworkx-api",
  "description": "API page for JSNetworkX",
  "scripts": {
    "start": "static website/",
    "build": "NODE_ENV=production browserify src/app.jsx -t babelify | uglifyjs -mc > website/bundle.js",
    "watch": "watchify src/app.jsx -t babelify -o website/bundle.js -v",
    "gen-api": "node buildDocs"
  },
  "author": "Felix Kling",
  "license": "BSD",
  "devDependencies": {
    "babelify": "^6",
    "browserify": "^9.0.7",
    "node-static": "^0.7.6",
    "uglify-js": "^2.4.17",
    "watchify": "^3"
  },
  "dependencies": {
    "acorn-babel": "^0.11.1-38",
    "body-parser": "^1.9.2",
    "catharsis": "^0.8.3",
    "docblock-parser": "^0.1.3",
    "express": "^4.10.2",
    "globby": "^1",
    "katex": "Khan/KaTex#v0.3.0",
    "kramed": "^0.4.3",
    "lodash": "^3",
    "promise": "^6.0.1",
    "react": "^0.13",
    "react-router": "^0.13",
    "recast": "^0.10",
    "rimraf": "^2.2.8",
    "semver": "^4.1.0",
    "whatwg-fetch": "^0.7.0"
  },
  "jsnx": {
    "repo": "./jsnetworkx/",
    "versions": [
      "v0.3.0"
    ],
    "versionsFile": "./website/versions.json",
    "versionsDir": "./website/versions"
  }
}
